<SIMPLE NAME="PC" PREFERREDCYCLELENGTH="10" PREFERREDNUMBERFORMAT="Hexadecimal">
	<FORMS>
		<RECTANGLE POSX="-10" POSY="-10" WIDTH="20" HEIGHT="20" COLOR="#0000C0"/>
	</FORMS>
	<IO>
		<INPUT NAME="In" BITS="32" SIGNED="false" POSX="-10" POSY="0" RELATIVELABELPOSX="-40" RELATIVELABELPOSY="0"  COLOR="#FFFF33"/>
		<OUTPUT NAME="Out" BITS="32" SIGNED="false" POSX="10" POSY="0" RELATIVELABELPOSX="35" RELATIVELABELPOSY="0"  COLOR="#FFFF33"/>
	</IO>
	<INTERNALS DELAY="10">
		<ACTION EVENT="CLOCK_RISING">
			Out = In;&#9;&#9;&#10;
		</ACTION>
	</INTERNALS>
</SIMPLE>
